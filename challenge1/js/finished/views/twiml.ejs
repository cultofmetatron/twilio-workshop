<?xml version="1.0" encoding="UTF-8"?>
<Response>
	<% if(digits) { %>
		<% if(digits == '1') { %>
		<Say>Now connecting to sales.</Say>
		<Dial>14155551212</Dial>
		<% } else { %>
		<Say>Now connecting to services.</Say>
		<Dial>14155551212</Dial>
		<% } %>
	<% } else { %>
	<Gather>
		<Say>Thanks for calling Acme LLC!</Say>
		<Say voice="woman">
			For the Sales department, press 1.
			For service, press 2.
		</Say>
	</Gather>
	<% } %>
</Response>